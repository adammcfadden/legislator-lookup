<div class="panel panel-default">
  <div class="panel-heading text-center">
    <h3>Your Legislators</h3>
  </div>
  <div class="panel-body">
    <ul>
      {{#each legislator in model}}
        <li>
          <span class="legislator-name"> {{legislator.first_name}} {{legislator.last_name}} </span> <br>
          Contact: <a href={{legislator.contact_form}}>{{legislator.contact_form}}</a>
        </li>
      {{/each}}
    </ul>
  </div>
</div>
